@model IEnumerable<MVC2024.Controllers.VehiculoController.VehiculoTotal>

@{
    ViewData["Title"] = "ListWithProcedure";
}

<h1>List with Procedure</h1>
<p>This is beacuse of the use of a created procedure in SQL MANAGEMENT</p>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Marca")
            </th>
            <th>
                @Html.DisplayName("Modelo")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Matricula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Color)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NomMarca)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NomSerie)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Matricula)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Color)
                </td>
            </tr>
        }
    </tbody>
</table>
